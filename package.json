{
	"name": "@renatodellosso/typed-api-client",
	"version": "1.0.11",
	"description": "",
	"author": "Renato Dell'Osso",
	"license": "MIT",
	"scripts": {
		"build": "tsc & tsc --declaration",
		"test": "jest --passWithNoTests",
		"lint": "eslint . --max-warnings=0",
		"prettier-fix": "prettier --write ./src/**/*.ts",
		"prettier-check": "prettier --check ./src/**/*.ts"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Decatur-Robotics/npm-package-template.git"
	},
	"keywords": [
		"typescript",
		"api",
		"client",
		"typed",
		"schema",
		"zod",
		"typesafe"
	],
	"files": [
		"dist/**/*"
	],
	"typesVersions": {
		"*": {
			"*": [
				"dist/*"
			]
		}
	},
	"exports": {
		"./*": "./dist/*.js"
	},
	"main": "dist/index.js",
	"devDependencies": {
		"@types/jest": "^29.5.14",
		"eslint": "^9.17.0",
		"jest": "^29.7.0",
		"prettier": "^3.4.2",
		"ts-jest": "^29.2.5",
		"ts-node": "^10.9.2"
	},
	"dependencies": {
		"@standard-schema/spec": "^1.0.0",
		"zod": "^4.1.12"
	}
}
